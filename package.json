{
  "name": "@nonsoo/prisma-mermaid",
  "version": "0.1.3",
  "description": "A Prisma generator that generates Mermaid Class or ER diagrams from your Prisma schema.",
  "main": "build/index.js",
  "bin": {
    "prisma-mermaid": "./build/lib/PrismaMermaidGenerators/bin.cjs"
  },
  "types": "build/lib/PrismaMermaidGenerators/index.d.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc",
    "test": "vitest",
    "test:ci": "vitest run",
    "prettier:fix": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "keywords": [
    "prisma",
    "generator",
    "diagrams",
    "mermaid",
    "erd",
    "Entity Relationship Diagram",
    "typescript"
  ],
  "author": "Nonso O",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/nonsoo/prisma-mermaid.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs",
      "default": "./build/index.js"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.39.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.1",
    "globals": "^16.5.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.10"
  },
  "peerDependencies": {
    "@prisma/client": "^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "@prisma/internals": "^7.0.0",
    "js-yaml": "^4.1.1"
  }
}
